#https://cgit.freedesktop.org/xorg/proto/x11proto/tree/XF86keysym.h

super + Escape
    pkill -USR1 -x sxhkd;\
    notify-send "sxhkd restarted"

super + shift + v
    clipmenu

super + shift + t
    gnome-pomodoro --no-default-window --pause-resume

super + shift + p
    rofimoji -a clipboard

super + y
    dwmc togglescratch {0}

super + z
    dwmc togglescratch {1}

super + o
    dwmc togglescratch {2}

super + q
    dwmc togglescratch {3}

super + shift + c
    rofi -show calc -modi calc -no-show-match -no-sort -calc-command "echo -n '{result}' | xclip"
super + c
  $HOME/.config/sxhkd/rcode.sh

super + a
    dwmc togglescratch {5}
super + n
    dwmc togglescratch {6}
super + u
    dwmc togglescratch {7}

super + shift + e
     ~/.config/sxhkd/powermenu.sh

super + p
    rofi -show
XF86Stop
    pactl set-sink-mute 0 toggle
XF86Back
    pactl set-sink-volume 0 -5%
XF86Forward
    pactl set-sink-volume 0 +5%
XF86HomePage
    playerctl next
XF86Search
    playerctl previous
XF86Favorites
    playerctl play-pause
XF86Mail
   slock
XF86MyComputer
   rofi -show power-menu -modi power-menu:rofi-power-menu
XF86AudioPrev
    playerctl previous
XF86AudioMute
    pactl set-sink-mute 0 toggle
XF86AudioLowerVolume
    pactl set-sink-volume 0 -5%
XF86AudioRaiseVolume
    pactl set-sink-volume 0 +5%
XF86AudioNext
    playerctl next
XF86AudioPlay
    playerctl play-pause
XF86MonBrightnessUp
   xbacklight -inc 10; notify-send 'brightness up'
 
XF86MonBrightnessDown
   xbacklight -dec 10; notify-send 'brightness down'
(END)
