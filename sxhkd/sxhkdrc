#https://cgit.freedesktop.org/xorg/proto/x11proto/tree/XF86keysym.h

super + Escape
    pkill -USR1 -x sxhkd;\
    notify-send "sxhkd restarted"

super + shift + v
    rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'
super + y
    dwmc togglescratch {0}

super + z
    dwmc togglescratch {1}

super + r
    dwmc togglescratch {2}

super + q
    dwmc togglescratch {3}

rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'

super + a
    dwmc togglescratch {4}

super + e
    dwmc togglescratch {5}
super + n
    dwmc togglescratch {6}
super + w
    dwmc togglescratch {7}

super + s
    sh ~/.config/dwm/bindings.sh

super + p
    rofi -show
super + shift + Return
    kitty
XF86Stop
    pactl set-sink-mute 0 toggle
XF86Back
    pactl set-sink-volume 0 -5%
XF86Forward
    pactl set-sink-volume 0 +5%
XF86HomePage
    playerctl next
XF86Search
    playerctl previous
XF86Favorites
    playerctl play-pause
XF86Mail
    slock
XF86MyComputer
    sudo poweroff
XF86AudioPrev
    playerctl previous
XF86AudioMute
    pactl set-sink-mute 0 toggle
XF86AudioLowerVolume
    pactl set-sink-volume 0 -5%
XF86AudioRaiseVolume
    pactl set-sink-volume 0 +5%
XF86AudioNext
    playerctl next
XF86AudioPlay
    playerctl play-pause
